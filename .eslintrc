{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
    },
    "parserOptions": {
        "ecmaVersion": 2021
    },
    "extends": [
        "airbnb-base",
    ],
    "rules": {
        "one-var": "off",
        "react/jsx-no-bind": "off",
        "react/jsx-boolean-value": "off",
        "jsx-a11y/href-no-hash": "off",
        "camelcase": "off",
        "no-console": "off",
    },
}
